{
  "name": "brandonlind",
  "version": "1.0.0",
  "description": "The personal site for Brandon Lind.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ripvanbl/brandonlind.git"
  },
  "keywords": [
    "brandon",
    "lind"
  ],
